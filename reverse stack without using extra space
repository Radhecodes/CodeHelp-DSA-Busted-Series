void insertatbottom(stack<int> &stack, int num){
    if(stack.empty()){
        stack.push(num);
        return;
    }
    int n=stack.top();
    stack.pop();
    insertatbottom(stack,num);
    stack.push(n);
    return;
}
void reverseStack(stack<int> &stack) {
    if(stack.empty()){
        return;
    }
    int num=stack.top();
    stack.pop();
    reverseStack(stack);
    insertatbottom(stack,num);
    return;
}
